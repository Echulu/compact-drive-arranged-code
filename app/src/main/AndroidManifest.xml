<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.compactdrive" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Central.Central"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoogleDrive.GetCode_GoogleDrive"
            android:label="GetCode_GoogleDrive"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".AppUtils.fileDownload"
            android:label="fileDownload"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".OneDrive.GetCode_OneDrive"
            android:label="GetCode_OneDrive"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".OneDrive.OneDrive_SignOut"
            android:label="OneDrive_SignOut"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".AppUtils.About"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".GoogleDrive.GoogleDrive_SignOut" >
        </activity>
    </application>

</manifest>
